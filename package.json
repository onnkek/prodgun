{
  "name": "prodgun",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run start:front\" \"npm run start:back\"",
    "start:front": "webpack serve --env port=3000 --host localhost",
    "start:back": "node ./json-server/index.js",
    "build:prod": "webpack --env mode=production apiUrl=https://prodgun-backend.vercel.app",
    "build:dev": "webpack --env mode=development",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "storybook": "storybook dev -p 6006 -c ./config/storybook",
    "storybook:build": "storybook build -c ./config/storybook",
    "test:unit": "jest --config ./config/jest/jest.config.ts",
    "test:ui": "npx loki test",
    "test:ui:ci": "npx loki --requireReference --reactUri file:./storybook-static",
    "test:ui:ok": "npx loki approve",
    "test:ui:report": "npm run test:ui:json && npm run test:ui:html",
    "test:ui:json": "node scripts/generate-visual-json-report.js",
    "test:ui:html": "reg-cli --from .loki/report.json --report .loki/report.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onnkek/prodgun.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/onnkek/prodgun/issues"
  },
  "homepage": "https://github.com/onnkek/prodgun#readme",
